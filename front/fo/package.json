{
  "engines": {
    "node": "^18.0.0"
  },
  "browserslist": [
    "last 2 version",
    "android >= 4.4",
    "IE >= 11"
  ],
  "imports": {
    "#ustra/core": "@ustra/core/src",
    "#ustra/core/*": "@ustra/core/src/*",
    "#ustra/h3": "@ustra/h3/src",
    "#ustra/h3/*": "@ustra/h3/src/*",
    "#ustra/nuxt": "@ustra/nuxt/src",
    "#ustra/nuxt/*": "@ustra/nuxt/src/*",
    "#ustra/nuxt-vuetify": "@ustra/nuxt-vuetify/src",
    "#ustra/nuxt-vuetify/*": "@ustra/nuxt-vuetify/src/*"
  },
  "scripts": {
    "dev": "cross-env CONFIG_ENV=local PORT=9912 nuxi dev",
    "build:dev": "cross-env CONFIG_ENV=dev nuxi build",
    "build:prd": "cross-env CONFIG_ENV=prd nuxi build",
    "start:dev": "cross-env PORT=9912 node .output/server/index.mjs",
    "start:prd": "cross-env PORT=9912 node .output/server/index.mjs"
  },
  "description": "USTRA Node framework3 Sample FO",
  "license": "MIT",
  "homepage": "http://guide.ustraframework.kro.kr/",
  "resolutions": {
    "vuetify": "3.4.1"
  },
  "dependencies": {
    "@ustra/core": "3.2.5-stable",
    "@ustra/h3": "3.2.5-stable",
    "@ustra/nuxt": "3.2.5-stable",
    "@ustra/nuxt-ckeditor5": "3.2.5-stable",
    "@ustra/nuxt-wijmo": "3.2.5-stable",
    "@ustra/nuxt-vuetify": "3.2.5-stable",
    "@pinia/nuxt": "0.5.5",
    "animejs": "3.2.2"
  }
}
