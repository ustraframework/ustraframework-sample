{
  "engines": {
    "node": "^18.0.0"
  },
  "browserslist": [
    "last 2 version",
    "android >= 4.4",
    "IE >= 11"
  ],
  "imports": {
    "#ustra/core": "@ustra/core/src",
    "#ustra/core/*": "@ustra/core/src/*",
    "#ustra/h3": "@ustra/h3/src",
    "#ustra/h3/*": "@ustra/h3/src/*",
    "#ustra/nuxt": "@ustra/nuxt/src",
    "#ustra/nuxt/*": "@ustra/nuxt/src/*",
    "#ustra/nuxt-vuetify": "@ustra/nuxt-vuetify/src",
    "#ustra/nuxt-vuetify/*": "@ustra/nuxt-vuetify/src/*"
  },
  "scripts": {
    "dev": "cross-env CONFIG_ENV=dev NODE_ENV=development NODE_OPTIONS='--stack-trace-limit=100' nuxi dev --dotenv .env.dev",
    "build:dev": "cross-env CONFIG_ENV=dev nuxi build",
    "build:prd": "cross-env CONFIG_ENV=prd nuxi build",
    "start:dev": "node .output/server/index.mjs",
    "start:prd": "node .output/server/index.mjs",
    "generate": "cross-env CONFIG_ENV=dev nuxi generate",
    "preview": "cross-env CONFIG_ENV=dev nuxi preview",
    "clean": "nuxi cleanup",
    "prepare": "nuxi prepare",
    "init": "rm -rf node_modules && yarn install"
  },
  "name": "@ustra/demo-nuxt-wijmo-management",
  "version": "3.5.0-snapshot",
  "description": "USTRA Node framework3 Demo",
  "author": "Roy Lee <keymasroy@gsitm.com>",
  "license": "MIT",
  "homepage": "http://guide.ustraframework.kro.kr/",
  "resolutions": {
    "vuetify": "3.4.1"
  },
  "dependencies": {
    "@ustra/core": "3.2.5-stable",
    "@ustra/h3": "3.2.5-stable",
    "@ustra/nuxt": "3.2.5-stable",
    "@ustra/nuxt-ckeditor5": "3.2.5-stable",
    "@ustra/nuxt-wijmo": "3.2.5-stable",
    "@ustra/nuxt-vuetify": "3.2.5-stable",
    "@pinia/nuxt": "0.5.5",
    "animejs": "3.2.2",
    "nuxt-cache-ssr": "^1.1.0"
  }
}
