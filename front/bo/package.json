{
  "engines": {
    "node": "^18.0.0"
  },
  "browserslist": [
    "last 2 version",
    "android >= 4.4",
    "IE >= 11"
  ],
  "imports": {
    "#ustra/core": "@ustra/core/src",
    "#ustra/core/*": "@ustra/core/src/*",
    "#ustra/h3": "@ustra/h3/src",
    "#ustra/h3/*": "@ustra/h3/src/*",
    "#ustra/nuxt": "@ustra/nuxt/src",
    "#ustra/nuxt/*": "@ustra/nuxt/src/*",
    "#ustra/nuxt-wijmo": "@ustra/nuxt-wijmo/src",
    "#ustra/nuxt-wijmo/*": "@ustra/nuxt-wijmo/src/*",
    "#ustra/nuxt-ckeditor5": "@ustra/nuxt-ckeditor5/src",
    "#ustra/nuxt-ckeditor5/*": "@ustra/nuxt-ckeditor5/src/*"
  },
  "scripts": {
    "dev": "cross-env CONFIG_ENV=local PORT=9911 nuxi dev",
    "generate:dev": "cross-env CONFIG_ENV=dev nuxi generate",
    "generate:prd": "cross-env CONFIG_ENV=prd nuxi generate"
  },
  "description": "USTRA Node framework3 Sample BO",
  "license": "MIT",
  "homepage": "http://guide.ustraframework.kro.kr/",
  "resolutions": {
    "vuetify": "3.4.1"
  },
  "dependencies": {
    "@ustra/core": "3.2.5-stable",
    "@ustra/h3": "3.2.5-stable",
    "@ustra/nuxt": "3.2.5-stable",
    "@ustra/nuxt-ckeditor5": "3.2.5-stable",
    "@ustra/nuxt-wijmo": "3.2.5-stable",
    "@pinia/nuxt": "0.5.5"
  }
}
