{
  "private": true,
  "workspaces": [
    "./cmm",
    "./fo",
    "./bo"
  ],
  "engines": {
    "node": "^18.0.0"
  },
  "browserslist": [
    "last 2 version",
    "android >= 4.4",
    "IE >= 11"
  ],
  "scripts": {
    "install:fo": "cd ./fo && yarn install",
    "dev:fo": "cd ./fo && yarn run dev",
    "generate:dev:fo": "cd ./fo && yarn run build:dev",
    "generate:prd:fo": "cd ./fo && yarn run build:prd",
    "install:bo": "cd ./bo && yarn install",
    "dev:bo": "cd ./bo && yarn run dev",
    "generate:dev:bo": "cd ./bo && yarn run generate:dev",
    "generate:prd:bo": "cd ./bo && yarn run generate:prd",
    "create-off-pack": "rm -rf node_modules && rm yarn.lock && rm -rf npm_packages/* && yarn cache clean && yarn install"
  },
  "dependencies": {
  }
}
